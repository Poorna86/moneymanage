{
    "presets":[
        [
            "@babel/preset-env",
            {
              "targets": {
                "node": "10"
              }
            }
          ],
          "@babel/preset-react"
        ],
    "plugins":[
        "transform-class-properties",
        "transform-object-rest-spread",
        ["@babel/plugin-transform-runtime",
            {
              "regenerator": true
            }
        ]
    ]    
}